    "sp = \"@#$%&*}{_-\"\n",
    "password = num+alph+capalp+sp\n",
    "dig = 15\n",
    "\n",
    "pas = \"\".join(random.sample(password,dig))\n",
    "print(\"your password is :  \",pas)\n"
